{
  "name": "solid-ts-todomvc",
  "version": "0.1.0",
  "description": "TodoMVC implemented in Typescript using Solid-js",
  "scripts": {
    "start": "webpack-dev-server --progress --color --mode development --config webpack.config",
    "build": "webpack --color --mode production --config webpack.config.js",
    "build:dev": "webpack --color --mode development --config webpack.config.js",
    "build:stats": "webpack --color --mode production --config webpack.config.js --json > stats.json",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "lint": "eslint ./",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "solid",
    "admin",
    "typescript"
  ],
  "author": "Roman Friedman",
  "license": "MIT",
  "homepage": "https://github.com/friedrom/solid-ts-todomvc",
  "repository": {
    "type": "git",
    "url": "https://github.com/friedrom/solid-ts-todomvc"
  },
  "private": true,
  "test": "jest",
  "browserslist": [
    "defaults",
    "not ie <= 12",
    "not ie_mob <= 12"
  ],
  "browser": "dist/lib.js",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "bulma": "^0.8.0",
    "idb": "^4.0.5",
    "solid-js": "^0.15.5",
    "todomvc-app-css": "^2.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime-corejs3": "^7.7.7",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@types/jest": "^24.0.21",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "axe-core": "3.3.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-dom-expressions": "^0.14.11",
    "babel-preset-solid": "^0.15.4",
    "core-js": "^3.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cypress": "3.6.0",
    "cypress-axe": "0.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-mocha": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.2",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "24.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser": "^4.3.9",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "typescript": "^3.7.0",
    "uglify-js": "^3.6.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "3.9.0"
  }
}
