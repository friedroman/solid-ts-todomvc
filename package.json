{
  "name": "solid-ts-todomvc",
  "version": "0.1.1",
  "description": "TodoMVC implemented in Typescript using Solid-js",
  "scripts": {
    "start": "webpack-dev-server --progress --color --mode development --config webpack.config",
    "build": "webpack --color --mode production --config webpack.config.js",
    "build:dev": "webpack --color --mode development --config webpack.config.js",
    "build:stats": "webpack --color --mode production --config webpack.config.js --json > stats.json",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "lint": "eslint ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "browserlist": "browserslist --update-db"
  },
  "keywords": [
    "solid",
    "admin",
    "typescript"
  ],
  "author": "Roman Friedman",
  "license": "MIT",
  "homepage": "https://github.com/friedrom/solid-ts-todomvc",
  "repository": {
    "type": "git",
    "url": "https://github.com/friedrom/solid-ts-todomvc"
  },
  "private": true,
  "test": "jest",
  "browserslist": [
    "defaults and > 1%, not ie 11, not ie_mob 11, not op_mini all"
  ],
  "browser": "dist/lib.js",
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "bulma": "^0.9.2",
    "idb": "^7.0.2",
    "solid-js": "^1.4.7",
    "todomvc-app-css": "^2.3.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/runtime-corejs3": "^7.18.9",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "axe-core": "^4.4.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-jsx-dom-expressions": "^0.33.12",
    "babel-preset-solid": "^1.4.6",
    "core-js": "^3.23.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.3.0",
    "cypress-axe": "1.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser": "^5.14.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "typescript": "4.3.5",
    "uglify-js": "^3.16.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "packageManager": "yarn@3.2.1"
}
