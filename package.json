{
  "name": "solid-ts-todomvc",
  "version": "0.1.1",
  "description": "TodoMVC implemented in Typescript using Solid-js",
  "scripts": {
    "start": "webpack-dev-server --progress --color --mode development --config webpack.config",
    "build": "webpack --color --mode production --config webpack.config.js",
    "build:dev": "webpack --color --mode development --config webpack.config.js",
    "build:stats": "webpack --color --mode production --config webpack.config.js --json > stats.json",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "lint": "eslint ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "browserlist": "browserslist --update-db"
  },
  "keywords": [
    "solid",
    "admin",
    "typescript"
  ],
  "author": "Roman Friedman",
  "license": "MIT",
  "homepage": "https://github.com/friedrom/solid-ts-todomvc",
  "repository": {
    "type": "git",
    "url": "https://github.com/friedrom/solid-ts-todomvc"
  },
  "private": true,
  "test": "jest",
  "browserslist": [
    "defaults and > 1%, not ie 11, not ie_mob 11, not op_mini all"
  ],
  "browser": "dist/lib.js",
  "dependencies": {
    "@babel/runtime": "^7.23.6",
    "bulma": "^0.9.4",
    "idb": "^8.0.0",
    "lorem-ipsum": "^2.0.8",
    "solid-js": "^1.8.7",
    "todomvc-app-css": "^2.4.3",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/runtime-corejs3": "^7.23.6",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "axe-core": "^4.8.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-jsx-dom-expressions": "^0.37.9",
    "babel-preset-solid": "^1.8.6",
    "core-js": "^3.34.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cypress": "^13.6.1",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fork-ts-checker-notifier-webpack-plugin": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser": "^5.26.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "packageManager": "yarn@4.0.2"
}
