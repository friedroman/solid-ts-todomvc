{
  "name": "solid-ts-todomvc",
  "version": "0.1.0",
  "description": "TodoMVC implemented in Typescript using Solid-js",
  "scripts": {
    "start": "webpack-dev-server --progress --color --mode development --config webpack.config",
    "build": "webpack --color --mode production --config webpack.config.js",
    "build:dev": "webpack --color --mode development --config webpack.config.js",
    "build:stats": "webpack --color --mode production --config webpack.config.js --json > stats.json",
    "cypress:version": "cypress version",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headed": "cypress run --headed",
    "lint": "eslint ./",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "solid",
    "admin",
    "typescript"
  ],
  "author": "Roman Friedman",
  "license": "MIT",
  "homepage": "https://github.com/friedrom/solid-ts-todomvc",
  "repository": {
    "type": "git",
    "url": "https://github.com/friedrom/solid-ts-todomvc"
  },
  "private": true,
  "test": "jest",
  "browserslist": [
    "defaults and > 1%, not ie 11, not ie_mob 11, not op_mini all"
  ],
  "browser": "dist/lib.js",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "bulma": "^0.9.0",
    "idb": "^5.0.4",
    "solid-js": "^0.19.0",
    "todomvc-app-css": "^2.3.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime-corejs3": "^7.11.2",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "axe-core": "4.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-jsx-dom-expressions": "^0.20.0",
    "babel-preset-solid": "^0.19.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "cypress": "^4.12.1",
    "cypress-axe": "0.8.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-notifier-webpack-plugin": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "terser": "^5.2.1",
    "ts-jest": "26.3.0",
    "ts-loader": "8.0.3",
    "typescript": "4.0.2",
    "uglify-js": "^3.10.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
